{
  "name": "vscode-ext-quit-text-tabs",
  "version": "0.1.2",
  "displayName": "Quit text tabs",
  "description": "Quit text tabs",
  "keywords": [],
  "author": "hankei6km <hankei6km@gmail.com> (https://github.com/hankei6km)",
  "publisher": "hankei6km",
  "repository": {
    "type": "git",
    "url": "https://github.com/hankei6km/vscode-ext-quit-text-tabs"
  },
  "bugs": {
    "url": "https://github.com/hankei6km/vscode-ext-quit-text-tabs/issues"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "main": "./out/extension.js",
  "browser": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "extension.quitTextTabs",
        "title": "Quit Text Tabs",
        "category": "View"
      }
    ]
  },
  "scripts": {
    "build": "npm run compile",
    "vscode:prepublish": "npm run clean && npm run build && bash scripts/gen-changelog.sh",
    "build:rust": "cargo build --target wasm32-wasi --release && cp target/wasm32-wasi/release/rust-hello.wasm wasm/bin/hello.wasm",
    "compile": "node esbuild.config.mjs",
    "test": "npm run test:unit && npm run test:electron && npm run test:web",
    "pretest": "",
    "test:unit": "mocha --require ts-node/register --require source-map-support/register --watch-extensions ts --ui tdd src/test/unit/**/*.test.ts",
    "test:electron": "xvfb-run node ./out/test/runTest.js",
    "pretest:electron": "node esbuild.test-electron.config.mjs",
    "test:web": "vscode-test-web --headless=true --coi true --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=out/test/web/index.js",
    "pretest:web": "node esbuild.test-web.config.mjs",
    "lint:type-check": "tsc --noEmit",
    "clean": "rimraf out/",
    "serve": "serve --cors -l 5000 --ssl-cert $HOME/certs/localhost.pem --ssl-key $HOME/certs/localhost-key.pem -c ./configs/serve.json .",
    "serve:http": "serve --cors -l 5000"
  },
  "dependencies": {},
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@playwright/test": "^1.36.1",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.0",
    "@types/vscode": "^1.81.0",
    "@vscode/test-electron": "^2.3.4",
    "@vscode/test-web": "^0.0.44",
    "@vscode/vsce": "^2.20.1",
    "assert": "^2.0.0",
    "esast-util-from-js": "^2.0.1",
    "esbuild": "^0.18.19",
    "estree-util-to-js": "^2.0.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.1",
    "serve": "^14.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
